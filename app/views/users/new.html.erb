<%= form_for @user do |f| %>

  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :password %>
  <br>
  <%= f.text_field :password %>
  <br><br>
  <%= f.label :password_confirmation %>
  <br>
  <%= f.text_field :password_confirmation %>
  <br><br>
  <%= f.check_box :admin %>&nbsp;&nbsp;&nbsp;<%= f.label :admin %>
  <br><br><br><br>
  <%= f.label :height %>
  <br>
  <%= f.text_field :height %>
  <br><br>
  <%= f.label :nausea %>
  <br>
  <%= f.text_field :nausea %>
  <br><br>
  <%= f.label :happiness %>
  <br>
  <%= f.text_field :happiness %>
  <br><br>
  <%= f.label :tickets %>
  <br>
  <%= f.text_field :tickets %>
  <br><br>

  <%= f.submit "Create User" %>

<% end %>
